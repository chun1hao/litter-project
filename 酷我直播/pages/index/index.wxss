.top-nav{
  width: 100%;
  height: 103rpx;
  background: #ECDBD4;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
}
.top-nav text{
  width: 143rpx;
  height: 58rpx;
  display: inline-block;
  background: #e29142;
  color: #fff;
  font-size: 27rpx;
  line-height: 58rpx;
  margin-top:23rpx; 
}
.top-nav .left{
  border-radius:27rpx 0 0 27rpx;
}
.top-nav .right{
  border-radius:0 27rpx 27rpx 0;
}
.top-nav .active{
  background: #ED4650;
}

.middle-nav{
  width: 100%;
  height: 82rpx;
  display: flex;
  justify-content: space-around; 
  position: absolute;
  top: 103rpx;
  left: 0;
}
.middle-nav text{
  display: inline-block;
  text-align: center;
  line-height: 82rpx;
  font-size: 30.5rpx;
  width: 33.3%;
  border-bottom: 1.17rpx solid #d9d9d9;
}
.middle-nav .active{
  color: #e29142;
  border-bottom: 5.85rpx solid #e29142;
  line-height: 77.22rpx;
}
.list .people{
  float: right;
  color: #ccc;
  line-height: 50rpx;
}
.main{
  overflow: auto;
  position: absolute;
  top: 185rpx;
  left: 0;
  right: 0;
  bottom: 103rpx;
}
.swip{
  width: 750rpx;
  height: 100%; 
}
.list{
  width: 368.55rpx;
  height: 438.75rpx;
  border:1rpx solid #d9d9d9;
  margin-top: 10rpx;
}
.img{
  width: 368.55rpx;
  height: 368.55rpx;
  border-bottom: 1rpx solid #d9d9d9;
}
.list text{
  font-size: 27rpx;
  padding-left: 25rpx;
}
.item{
  overflow: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between; 
}
.bot-nav{
  width: 100%;
  height: 103rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  /*line-height: 103rpx;*/
  font-size: 27rpx;
  color: #dddddd;
  justify-content: space-between;
  border-top: 1rpx solid #d9d9d9;
}
.bot-nav view{
  text-align: center;
  margin-top: 8rpx;
}
.bot-nav .left,.bot-nav .right{
  width: 313rpx;
  display: flex;
  justify-content: space-around;
}
.bot-nav .car{
  position: absolute;
  width: 124rpx;
  height: 124rpx;
  border-radius: 62rpx;
  border: 1rpx solid #dedede;
  top: -24rpx;
  left: 50%;
  margin-left: -62rpx;
  background: #fff;
  text-align: center;
  line-height: 115rpx;
  margin-top: 0;
}
.car text{
   font-size: 61rpx;
}
.bot-nav .active{
  color: #e29142;
}
.more{
  display: flex;
  flex-direction:column;
  text-align: center;
  align-self: center;
  justify-content: center;
}
.refesh{
  display: flex;
  flex-direction:column;
  align-items: center;
  align-self: center;
  justify-content: center;
}




@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-xiangji:before { content: "\e605"; }

.icon-tuichu:before { content: "\e7c7"; }

.icon-wode:before { content: "\e649"; }

.icon-faxian:before { content: "\e615"; }

.icon-shouyeshouye:before { content: "\e600"; }